{
  "name": "AdBlocker",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["blocker.js"]
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "will@adblocking",
      "strict_min_version": "109.0"
    }
  }
}
  